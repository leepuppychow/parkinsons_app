<div class="header">
  <h1>Admin
    <%= link_to "Logout", logout_path, class: "nav-bar-links" %>
    <%= link_to "Admin Home", admin_welcome_index_path, class: "nav-bar-links" %>
  </h1>
</div>

<h2> All current therapists </h2>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Therapy Type</th>
    <th>Years Experience</th>
    <th>Patient Count</th>
  </tr>
<% @therapists.each do |therapist| %>
  <tr>
    <th><%= therapist.first_name %></th>
    <th><%= therapist.last_name %></th>
    <th><%= therapist.therapy_type %></th>
    <th><%= therapist.years_experience %></th>
    <th><%= therapist.patients.count %></th>
  </tr>
<% end %>
</table>
