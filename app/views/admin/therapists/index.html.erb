<div class="header">
  <h1>Admin: All Therapists
    <%= render partial: "admin/admin_header" %>
  </h1>
</div>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Specialty</th>
    <th>Patient Count</th>
  </tr>
<% @therapists.each do |therapist| %>
  <tr>
    <th><%= therapist.name %></th>
    <th><%= therapist.specialty %></th>
    <th><%= therapist.location%></th>
    <th><%= therapist.phone%></th>
    <th><%= therapist.patients.count %></th>
  </tr>
<% end %>
</table>
