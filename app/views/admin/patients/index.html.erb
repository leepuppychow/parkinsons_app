<div class="header">
  <h1>Admin
    <%= link_to "Logout", logout_path, class: "nav-bar-links" %>
    <%= link_to "Admin Home", admin_welcome_index_path, class: "nav-bar-links" %>
  </h1>
</div>

<h2> All current patients </h2>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Username</th>
    <th>Age</th>
    <th>Doctor</th>
    <th>User Type</th>
  </tr>
<% @patients.each do |patient| %>
  <tr>
    <th><%= patient.first_name %></th>
    <th><%= patient.last_name %></th>
    <th><%= patient.username %></th>
    <th><%= patient.age %></th>
    <th><%= patient.doctor_id %></th>
    <th><%= patient.role %></th>
  </tr>
<% end %>
</table>
