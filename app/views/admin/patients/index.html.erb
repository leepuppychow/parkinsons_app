<div class="header">
  <h1>Admin: All Patients
    <%= render partial: "admin/admin_header" %>
  </h1>
</div>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Username</th>
    <th>Age</th>
    <th>User Type</th>
  </tr>
<% @patients.each do |patient| %>
  <tr>
    <th><%= patient.first_name %></th>
    <th><%= patient.last_name %></th>
    <th><%= patient.username %></th>
    <th><%= patient.age %></th>
    <th><%= patient.role %></th>
  </tr>
<% end %>
</table>
