<div class="header">
  <h1> Login
  <%= link_to "Home", "/", class: "nav-bar-links"%>
  </h1>
</div>

<div class="column-container">
<table class="form-table">
<%= form_tag do %>
<tr>
  <th><%= label_tag :username %></th>
  <th><%= text_field_tag :username, "", class: "entry-form" %></th>
</tr>
<tr>
  <th><%= label_tag :password %></th>
  <th><%= password_field_tag :password, "", class: "entry-form" %></th>
</tr>
<tr>
  <th><%= submit_tag "Log In"%></th>
</tr>
<% end  %>

</table>
</div>
